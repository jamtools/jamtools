{
  "name": "jamtools-webapp",
  "scripts": {
    "dev": "npm run build -- --watch",
    "build": "esbuild src/index.tsx --outfile=dist/index.js --bundle --sourcemap=inline --define:process.env.WS_HOST=\"'$WS_HOST'\" --define:process.env.DATA_HOST=\"'$DATA_HOST'\" --target=es6",
    "build-saas": "npm run build",
    "build-snack": "esbuild ./snacks/root_mode_snack/root_mode_snack.tsx --outfile=dist/index.js --bundle",
    "check-types": "tsc --noEmit",
    "lint": "eslint --ext ts --ext tsx src/",
    "fix": "npm run lint -- --fix"
  },
  "dependencies": {
    "jamtools-core": "*",
    "react-router-dom": "^6.25.0"
  },
  "devDependencies": {
    "configs": "*"
  },
  "config": {
    "dir": "../../../configs"
  }
}
