{
  "name": "springboard-cli",
  "version": "0.10.0",
  "main": "index.js",
  "scripts": {
    "build": "NODE_MODULES_PARENT_FOLDER=../../../ npx tsx esbuild.ts",
    "build-saas": "DISABLE_IO=true NODE_MODULES_PARENT_FOLDER=../../../ npx tsx esbuild.ts",
    "build-with-cli": "NODE_MODULES_PARENT_FOLDER=../../../ npx tsx esbuild.ts",
    "run-node-local": "NODE_KV_STORE_DATA_FILE=kv_data.json node dist/node/dist/index.js",
    "dev": "npm run build -- --watch"
  },
  "dependencies": {
    "esbuild": "^0.23.0",
    "npm-run-all": "^4.1.5",
    "tsx": "^4.19.2",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "springboard": "0.10.0",
    "@springboardjs/platforms-node": "0.10.0",
    "@springboardjs/platforms-browser": "0.10.0",
    "springboard-server": "0.10.0"
  },
