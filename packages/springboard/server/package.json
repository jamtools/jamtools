  "version": "0.14.1",
  "main": "./index.ts",
  "module": "./index.ts",
  "files": [
    "index.ts",
    "src"
  ],
  "scripts": {
    "start": "WEBAPP_FOLDER=../cli/dist/browser node ../cli/dist/server/dist/local-server.cjs",
